{
  "name": "coffee-brew-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild index.js --bundle --platform=node --format=esm --sourcemap --outfile=dist/server.mjs --packages=external",
    "start": "node --enable-source-maps dist/server.mjs",
    "dev": "esbuild index.js --bundle --platform=node --format=esm --sourcemap --outfile=dist/server.mjs --packages=external --watch"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "awilix": "^12.0.5",
    "awilix-express": "^9.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "pino-http": "^10.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "esbuild": "^0.19.12",
    "nodemon": "^2.0.22"
  }
}
